{
    "name": "drupal/powerstack_profile",
    "type": "drupal-profile",
    "description": "Base installation profile for Power Stack",
    "keywords": [
        "Drupal"
    ],
    "license": "GPL-2.0+",
    "repositories": {
        "composer-2-workspaces": {
            "type": "vcs",
            "url": "git@github.com:xaviemirmon/composer-2-workspaces-plugin.git"
        }
    },
    "homepage": "https://www.drupal.org/project/powerstack_profile",
    "authors": [
        {
            "name": "Your name here",
            "homepage": "https://www.drupal.org/u/your_name_here",
            "role": "Maintainer"
        },
        {
            "name": "Contributors",
            "homepage": "https://www.drupal.org/node/NID/committers",
            "role": "Contributors"
        }
    ],
    "support": {
        "issues": "https://www.drupal.org/project/issues/powerstack_profile",
        "source": "http://cgit.drupalcode.org/powerstack_profile"
    },
    "require": {
        "drupal/gin": "^3.0",
        "drupal/powerstack_api": "dev-master",
        "drupal/powerstack_page_builder": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "tmdk/composer-workspaces-plugin": "dev-overrides"
    },
    "extra": {
